{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/Homepage.html" },
    { "src": "/(.*)", "dest": "/$1.html" }
  ],
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "assets/stylesheet/**/*.css", "use": "@vercel/static" },
    { "src": "assets/javascript/**/*.js", "use": "@vercel/static" },
    { "src": "assets/images/**/*", "use": "@vercel/static" }
  ]
}

