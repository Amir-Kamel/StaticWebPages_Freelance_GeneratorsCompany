{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/Pages/Homepage.html" },
    { "src": "/About", "dest": "/Pages/About.html" },
    { "src": "/Contact", "dest": "/Pages/Contact.html" },
    { "src": "/CustomerService", "dest": "/Pages/CustomerService.html" },
    { "src": "/FAQ", "dest": "/Pages/FAQ.html" },
    { "src": "/Footer", "dest": "/Pages/Footer.html" },
    { "src": "/header", "dest": "/Pages/header.html" },
    { "src": "/product", "dest": "/Pages/product.html" },
    { "src": "/Services", "dest": "/Pages/Services.html" },
    { "src": "/Pages/(.*)", "dest": "/Pages/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" }
  ],
  "builds": [
    { "src": "Pages/**/*.html", "use": "@vercel/static" },
    { "src": "assets/stylesheet/**/*.css", "use": "@vercel/static" },
    { "src": "assets/javascript/**/*.js", "use": "@vercel/static" },
    { "src": "assets/images/**/*", "use": "@vercel/static" }
  ]
}

